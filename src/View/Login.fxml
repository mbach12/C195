<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Login">
   <children>
      <Label fx:id="zoneLabel" layoutX="14.0" layoutY="5.0" text="Zone" textFill="#2f00bc" />
      <Label fx:id="loginLabel" layoutX="200.0" layoutY="44.0" text="LOGIN" textFill="#2f00bc">
         <font>
            <Font name="System Bold" size="40.0" />
         </font>
      </Label>
      <Label fx:id="usernameLabel" layoutX="187.0" layoutY="130.0" text="Username" textFill="#2f00bc">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <TextField fx:id="usernameText" layoutX="187.0" layoutY="148.0" onAction="#userName" promptText="Enter username" />
      <Label fx:id="passwordLabel" layoutX="186.0" layoutY="191.0" text="Password" textFill="#2f00bc">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwordText" accessibleRole="PASSWORD_FIELD" layoutX="188.0" layoutY="209.0" promptText="Enter password" />
      <Button fx:id="login" layoutX="244.0" layoutY="274.0" mnemonicParsing="false" onAction="#loginButtonHandler" text="LOGIN">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="exit" layoutX="252.0" layoutY="333.0" mnemonicParsing="false" onAction="#exitCLick" text="Exit">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
